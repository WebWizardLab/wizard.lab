---
import Container from '../shared/Container.astro'
import Title from '../shared/Title.astro'
const javascriptIcon =
  'https://res.cloudinary.com/dxsosgojl/image/upload/v1730983307/javascript_x6vn2v.svg'
const typescriptIcon =
  'https://res.cloudinary.com/dxsosgojl/image/upload/v1730983306/typescript-icon_u9ynwx.svg'
const htmlIcon =
  'https://res.cloudinary.com/dxsosgojl/image/upload/v1730983297/html_cuznh5.png'
const wordpressIcon =
  'https://res.cloudinary.com/dxsosgojl/image/upload/v1730983315/wordpress-icon_i2ykms.svg'
const woocommerceIcon =
  'https://res.cloudinary.com/dxsosgojl/image/upload/v1730983316/woocommerce-icon_cgb8tf.svg'
const mongoIcon = "https://res.cloudinary.com/dxsosgojl/image/upload/v1730983314/mongodb_ro41ec.svg"
const mysqlIcon = "https://res.cloudinary.com/dxsosgojl/image/upload/v1730983312/mysql_lf7tth.svg"
const awsIcon = "https://res.cloudinary.com/dxsosgojl/image/upload/v1730983311/aws_mdhsie.svg"
const nestIcon = "https://res.cloudinary.com/dxsosgojl/image/upload/v1730983308/nestjs_wnsrjz.svg"
const astroIcon = "https://res.cloudinary.com/dxsosgojl/image/upload/v1730983302/astro1_r2mqnj.png"
const cloudinaryIcon = "https://res.cloudinary.com/dxsosgojl/image/upload/v1730983301/cloudinary_gkc0zo.png"
const firebaseIcon = "https://res.cloudinary.com/dxsosgojl/image/upload/v1730983298/firebase1_smcihq.png"
const nodeIcon = "https://res.cloudinary.com/dxsosgojl/image/upload/v1730983296/node1_ukm71f.png"
const postgresIcon = "https://res.cloudinary.com/dxsosgojl/image/upload/v1730983293/postgresql_oswqzb.png"
const viteIcon = "https://res.cloudinary.com/dxsosgojl/image/upload/v1730983292/vite_s4ih6j.png"
const reactIcon = "https://res.cloudinary.com/dxsosgojl/image/upload/v1730984020/React_gqk1zr.png"
---

<section id='technologies'>
  <Container className='space-y-8'>
    <div class='text-center max-w-3xl mx-auto'>
      <Title> Nuestras tecnolog√≠as </Title>
    </div>
    <div class='flex justify-center flex-wrap gap-4'>
      <div
        class='dark:bg-tech-box-bg p-4 sm:p-5 rounded-xl border border-box-border group'
      >
        <img
          src={javascriptIcon}
          width='100'
          height='60'
          alt='javascript'
          class='h-7 sm:h-10 w-auto group-hover:scale-105'
        />
      </div>
      <div
        class='dark:bg-tech-box-bg p-4 sm:p-5 rounded-xl border border-box-border group'
      >
        <img
          src={typescriptIcon}
          width='100'
          height='60'
          alt='typescript'
          class='h-7 sm:h-10 w-auto group-hover:scale-105'
        />
      </div>
      <div
        class='dark:bg-tech-box-bg p-4 sm:p-5 rounded-xl border border-box-border group'
      >
        <img
          src={htmlIcon}
          width='100'
          height='60'
          alt='html-css'
          class='h-7 sm:h-10 w-auto group-hover:scale-105'
        />
      </div>
      <div
        class='dark:bg-tech-box-bg p-4 sm:p-5 rounded-xl border border-box-border group'
      >
        <img
          src={wordpressIcon}
          width='100'
          height='60'
